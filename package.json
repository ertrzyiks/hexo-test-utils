{
  "name": "hexo-test-utils",
  "version": "0.3.1",
  "description": "A set of utils for easy testing Hexo plugins",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "build": "./scripts/build.sh",
    "docs:generate": "jsdoc -r -c jsdoc.json",
    "docs:update": "gh-pages -d docs"
  },
  "ava": {
    "files": [
      "specs/**.spec.js",
      "specs/**/*.spec.js"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ertrzyiks/hexo-test-utils.git"
  },
  "peerDependencies": {
    "hexo": "3.x"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ertrzyiks/hexo-test-utils/issues"
  },
  "homepage": "https://github.com/ertrzyiks/hexo-test-utils#readme",
  "dependencies": {
    "bluebird": "^3.5.1",
    "stream-to-promise": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/register": "^7.0.0-beta.44",
    "ava": "^0.25.0",
    "docdash": "^0.4.0",
    "gh-pages": "^1.1.0",
    "hexo": "^3.6.0",
    "hexo-renderer-markdown-it": "^3.4.1",
    "jsdoc": "^3.5.5"
  }
}
